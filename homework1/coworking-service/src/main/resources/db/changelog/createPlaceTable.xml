<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
   <changeSet id="createPlaceTable" author="Seemer">
        <createTable schemaName="workSchema" tableName="Place">
            <column name="placeId" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="loginOwner" type="varchar(128)">
                <constraints nullable="false"/>
            </column>
            <column name="placeType" type="varchar(128)">
                <constraints nullable="false" />
            </column>
            <column name="seats" type="integer">
                <constraints nullable="false"/>
            </column>
        </createTable>
   </changeSet>
</databaseChangeLog>